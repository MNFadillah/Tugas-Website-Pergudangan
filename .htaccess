RewriteEngine On    # Turn on the rewriting engine
RewriteRule    ^([A-Za-z0-9-]+)/([A-Za-z0-9-]+)/([0-9-]+)?$     index.php?pages=$1&action=$2&id=$3    [NC,L] 
RewriteRule    ^([A-Za-z0-9-]+)/([A-Za-z0-9-]+)/?$     index.php?pages=$1&action=$2    [NC,L] 
RewriteRule    ^([A-Za-z0-9-]+)/?$     index.php?pages=$1    [NC,L]